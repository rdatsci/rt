#!/usr/bin/env Rscript

library("methods")
library("rt")
doc = "
Usage:
  rknit [--output=<format>] <file>

Options:
  --output=<format>  Output format (pdf*, html**, word)
                     * always used for Rnw files
                     ** default for Rmd files
"
rt:::cli.call("rknit", doc, commandArgs(TRUE))

# vim: ft=r
