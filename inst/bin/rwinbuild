#!/usr/bin/env Rscript

library("methods")
library("rt")
doc = "
Usage:
  rwinbuild [--devel] [--oldrelease] [<path>]
Options:
  --devel      Build with R-devel instead of R-release
  --oldrelease Build with R-oldrelease instead of R-release
"
rt:::cli.call("rwinbuild", doc, commandArgs(TRUE))

# vim: ft=r
