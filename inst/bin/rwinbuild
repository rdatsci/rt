#!/usr/bin/env Rscript

library("methods")
library("rtcl")
doc = "
Usage:
  rwinbuild [--devel --oldrel] [<path>]
Options:
  --devel   Build with R-devel instead of R-release
  --oldrel  Build with R-oldrelease instead of R-release
"
rtcl:::cli.call("rwinbuild", doc, commandArgs(TRUE))

# vim: ft=r
